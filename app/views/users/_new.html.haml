= form_for @user do |f|
  = f.text_field :username, placeholder: "User name"
  %br
  = f.text_field :email, placeholder: "johndoe@example.com"
  %br
  = f.password_field :password, placeholder: "Password"
  %br
  %input{alt: "Submit", height: "45%", src: "http://i.imgur.com/iRRFxmB.png", type: "image", width: "45%"}/
/ = link_to "sign in with google", "google_login"
